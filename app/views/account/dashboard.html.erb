<script type="text/javascript">
      function log(data){
        $('log').innerHTML += data + "<br/>"
      } 
    
    var friends_list = [<%= @friends.map { |friend| "'"+friend.account.name+"'"  }.join(',') %>]
    var current_user = {
      name : '<%= @current_user.name %>',
      id : <%= @current_user.id %>
    }
    friends_list.push(current_user.name)
    var sponsor,spongers
    
      Event.observe(window, 'load', function() {
        window.spongers = new Autocompleter('spongers', friends_list, {multiple: true, onChosen: function(){$('deal_submit').focus()}})
        window.sponsor = new Autocompleter('sponsor', friends_list, {onChosen: function(){window.spongers.input.focus()}})
        window.sponsor.push(current_user.name)
        
<%@defualt_spongers and @defualt_spongers.each do |sponger| %>
          window.spongers.push('<%= sponger.name %>')
<% end %>
      })
  var account_mapper = {<%= @friends.map { |friend| "'"+friend.account.name+"' : " +friend.account.id.to_s }.join(',') %>};
  account_mapper['<%= @current_user.name %>'] = <%= @current_user.id %>;

  function show_page(id){
    [$('feed_list'), $('new_deal'), $('new_friend')].each(function(page){
      if(page.id == id){
        if(page.style.display == 'none'){
          Effect.BlindDown(page.id)
        }
      }
      else if(page.style.display != 'none'){
        Effect.BlindUp(page.id)
      }
    })
  }
</script>

